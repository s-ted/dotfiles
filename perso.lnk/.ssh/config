# DevPod Start forex-trading-box.devpod
Host forex-trading-box.devpod
  ForwardAgent yes
  LogLevel error
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
  HostKeyAlgorithms rsa-sha2-256,rsa-sha2-512,ssh-rsa
  ProxyCommand "/usr/bin/devpod-cli" ssh --stdio --context default --user vscode forex-trading-box
  User vscode
# DevPod End forex-trading-box.devpod
# DevPod Start ferraillage.devpod
Host ferraillage.devpod
  ForwardAgent yes
  LogLevel error
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
  HostKeyAlgorithms rsa-sha2-256,rsa-sha2-512,ssh-rsa
  ProxyCommand "/usr/bin/devpod-cli" ssh --stdio --context default --user node ferraillage
  User node
# DevPod End ferraillage.devpod